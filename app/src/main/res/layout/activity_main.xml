<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="416dp"
        android:layout_height="283dp"
        android:background="@drawable/elem1"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498">

        <!-- Add toolbar items here -->
    </androidx.appcompat.widget.Toolbar>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        app:layout_constraintBottom_toBottomOf="parent"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar2"
                android:layout_width="382dp"
                android:layout_height="422dp"
                android:layout_marginTop="464dp"
                android:background="@drawable/elem2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.551"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <!-- Add toolbar items here -->
            </androidx.appcompat.widget.Toolbar>

            <TextView
                android:id="@+id/tvDate"
                android:layout_width="215dp"
                android:layout_height="39dp"
                android:fontFamily="@font/buffalo"
                android:gravity="center"
                android:textColor="#000000"
                android:textSize="35sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.001" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="107dp"
                android:layout_height="54dp"
                android:layout_marginTop="476dp"
                android:background="@drawable/underline"
                android:contentDescription="@string/title_image_description"
                android:fontFamily="@font/benedict"
                android:gravity="center"
                android:paddingBottom="4dp"
                android:text="Reminder"
                android:textColor="#FFFFFF"
                android:textSize="37.4sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.095"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="Reminder"
                tools:visibility="visible" />

            <com.example.myapplication.Clockie
                android:id="@+id/analogClock"
                android:layout_width="211dp"
                android:layout_height="216dp"
                android:layout_centerInParent="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.071" />

            <ImageButton
                android:id="@+id/imageButton"
                style="@android:style/Widget.ImageButton"
                android:layout_width="78dp"
                android:layout_height="73dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.049"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.325"
                app:srcCompat="@drawable/focus_ico" />

            <ImageButton
                android:id="@+id/imageButton2"
                style="@android:style/Widget.ImageButton"
                android:layout_width="87dp"
                android:layout_height="80dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.321"
                app:srcCompat="@drawable/class_ico" />

            <ImageButton
                android:id="@+id/imageButton3"
                style="@android:style/Widget.ImageButton"
                android:layout_width="82dp"
                android:layout_height="86dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.951"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.322"
                app:srcCompat="@drawable/track_ico" />

            <ImageButton
                android:id="@+id/imageButton4"
                style="@android:style/Widget.ImageButton"
                android:layout_width="77dp"
                android:layout_height="78dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.047"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.449"
                app:srcCompat="@drawable/health_ico" />

            <ImageButton
                android:id="@+id/imageButton5"
                style="@android:style/Widget.ImageButton"
                android:layout_width="81dp"
                android:layout_height="77dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.942"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.447"
                app:srcCompat="@drawable/res_ico" />

            <ImageButton
                android:id="@+id/imageButton6"
                style="@android:style/Widget.ImageButton"
                android:layout_width="89dp"
                android:layout_height="75dp"
                android:background="@null"
                android:contentDescription="@string/title_image_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.448"
                app:srcCompat="@drawable/cal_ico" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

    <TextView
        android:id="@+id/tvQuote"
        android:layout_width="352dp"
        android:layout_height="61dp"
        android:fontFamily="@font/merriweather"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.08" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="280dp"
        android:layout_height="49dp"
        android:fontFamily="@font/buffalo"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />
</androidx.constraintlayout.widget.ConstraintLayout>